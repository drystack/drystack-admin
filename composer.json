{
    "name": "drystack/drystack-admin",
    "authors": [
        {
            "name": "Lorenzo Cattaneo",
            "email": "info@lorenzocattaneo.com"
        }
    ],
    "license": "AGPL-3.0-only",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lorenzocattaneo/livewire-datatables"
        }
    ],
    "require": {
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0",
        "livewire/livewire": "^2.3",
        "drystack/components": "^0.1",
        "mediconesystems/livewire-datatables": "dev-master"

    },
    "autoload": {
        "psr-4": {
            "Drystack\\Admin\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Drystack\\Admin\\DrystackAdminServiceProvider"
            ]
        }
    }
}
